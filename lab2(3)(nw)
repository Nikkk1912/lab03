#include <iostream>
using namespace std;

int paint(char letter, int colum, int row)
{
	

	while (i = 0, i <= row, i++) 
	{
		while (t = 0, t <= colum, t++) 
		{
			cout << letter;
		}
	}
}


int main()
{
	char letter1, int colum1, int row1;

	cout << "Enter symbol: ";
	cin >> letter1;
	cout << "Emount of columns: ";
	cin >> colum1;
	cout << "Emount of rows: ";
	cin >> row1;

	paint(letter1, colum1, row1);

}
