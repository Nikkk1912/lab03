
#include <iostream>
using namespace std;

int paint(char letter, int colum, int row)
{
	

	for (int i = 0; i < row; i++) 
	{
		for (int t = 0; t < colum; t++) 
		{
			cout << letter;
		}
		cout << endl;
	}
	return 0;
}


int main()
{
	char letter1;
	int colum1, row1;

	cout << "Enter symbol: ";
	cin >> letter1;
	cout << "Emount of columns: ";
	cin >> colum1;
	cout << "Emount of rows: ";
	cin >> row1;

	paint(letter1, colum1, row1);

}
