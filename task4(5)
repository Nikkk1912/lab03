#include <iostream>
using namespace std;

double larger_of(double one, double two)
{
if (one > two) two = one;
else one = two;
cout << one << " and " << two;
};

int main()
{
double x, y;

cout << "Enter two numbers" << endl;
cin >> x >> y;
while(cin.good()){
    cout << "After larger_of function: " << larger_of(x, y) << endl;  

 cout << "Enter two numbers" << endl;
 cin >> x >> y;
}
return 0;
};
